{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server",
    "lint": "npx eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test dev http://localhost:5173 cy:open-e2e",
    "cy:coverage": "npx rimraf coverage && npx rimraf .nyc_output && start-server-and-test dev http://localhost:5173 cy:run-e2e && npx nyc report && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-redux": "^7.1.25",
    "bootstrap": "^5.2.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.16",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.0",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vi-fetch": "^0.8.0",
    "vite": "^4.0.4",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  }
}
